public class Paciente{

    //atributos
	private int codigo;
    private String nome;			//codigo,nome,idade,sexo
    private String sexo;
    private String idade;
 

    //construtores
    public   Paciente(int codigo,String nome, String sexo,String idade){
    	this.codigo=codigo;
        this.nome=nome;
        this.sexo=sexo;
        this.idade=idade;
      
    }
    
    public int getCodigo() {
    	return codigo;
    }
    public void setCodigo(int codigo) {
    	this.codigo=codigo;
    }
    
    
        
    //gets e sets
    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }
    public String getSexo(){
        return sexo;
    }
    public  void setSexo(String sexo){
        this.sexo=sexo;
    }
    
    
    public String getIdade(){
        return idade;
    }
    public void setIdade(String idade){
        this.idade=idade;
    }
   
   
    public String toString(){
        return this.codigo+ "\t" + this.nome+ "\t" +this.sexo+ "\t" +this.idade+"\n";

        }
    
    
}
